  "description": "FENIX Filter Utils",
  "main": "dist/fenix-ui-filter-utils.min.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mocha test/specs.js -w",
    "build": "NODE_ENV=production webpack  --display-error-details",
    "build:develop": "NODE_ENV=develop webpack  --display-error-details",
    "w-test": "mocha test/specs.js -w",
    "w-build": "SET NODE_ENV=production&& webpack --display-error-details",
    "w-build:develop": "SET NODE_ENV=develop&& webpack --display-error-details",
    "w-dev": "SET NODE_ENV=develop&& webpack-dev-server --display-error-details --progress --color --content-base dev  --inline --hot"

  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FENIX-Platform/fenix-ui-filter-utils.git"
  },
  "keywords": [
