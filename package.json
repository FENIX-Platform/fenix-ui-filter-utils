  "bugs": {
    "url": "https://github.com/FENIX-Platform/fenix-ui-filter-utils/issues"
  },
  "homepage": "https://github.com/FENIX-Platform/fenix-ui-filter-utils#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "clean-webpack-plugin": "^0.1.10",
    "jshint": "^2.9.3",
    "jshint-loader": "^0.8.3",
    "mocha": "^3.0.2",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.1"
  },
  "dependencies": {
    "handlebars": "^4.0.5",
